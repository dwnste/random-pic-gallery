{
    "env": {
        "browser": true,
        "node": true
    },
    "parser": "babel-eslint",
    "extends": ["airbnb"],
    "rules": {
        "default-case": 0,
        "react/jsx-indent": 0,
        "react/jsx-wrap-multilines": false,
        "react/jsx-filename-extension": false,
        "react/jsx-closing-tag-location": false,
        "react/jsx-indent-props": 4,
        "indent": ["error", 4],
        "jsx-a11y/no-static-element-interactions": false,
        "jsx-a11y/click-events-have-key-events": false
    }
}
